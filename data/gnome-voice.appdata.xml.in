<?xml version="1.0" encoding="UTF-8"?>
<application>
  <id type="desktop">gnome-voice.desktop</id>
  <_name>GNOME Voice for GNOME 42</_name>
  <_summary>Listen to Free Public Voice on GNOME 42</_summary>
  <licence>CC0</licence>
  <description>
    <p>
      GNOME Voice is a Free Software program that allows you easily
      participate on Free Public Voice broadcasts under GNOME 42.
    </p>
    <p>
      GNOME Voice is developed on the GNOME 42 desktop platform and
      it requires GTK+ 3.0, Clutter and GStreamer 1.0 for playback.
    </p>
    <p>    
    Enjoy Free Public Voice broadcasts under GNOME 42.
    </p>
  </description>
  <url type="homepage">https://wiki.gnome.org/Apps/Voice</url>
  <screenshots>
    <screenshot type="default">https://gitlab.gnome.org/ole/gnome-voice/data/screenshots/gnome-voice-screenshot.png</screenshot>
  </screenshots>
  <updatecontact>girl-list@gnome.org</updatecontact>
</application>
