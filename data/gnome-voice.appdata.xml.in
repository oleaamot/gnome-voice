<?xml version="1.0" encoding="UTF-8"?>
<application>
  <id type="desktop">gnome-voice.desktop</id>
  <_name>Voice</_name>
  <_summary>Listen to Free Public Voice on GNOME 48</_summary>
  <licence>CC0</licence>
  <description>
    <p>
      Voice is a Free Software program that allows you easily
      participate on Free Public Voice broadcasts under GNOME.
    </p>
    <p>
      Voice is developed on the GNOME 48 desktop platform and
      it requires GTK+ 3.0, Clutter and GStreamer 1.0 for playback
      and recording.
    </p>
    <p>    
    Enjoy Free Public Voice broadcasts under GNOME 48.
    </p>
  </description>
  <url type="homepage">https://wiki.gnome.org/Apps/Voice</url>
  <screenshots>
    <screenshot type="default">https://gitlab.gnome.org/ole/gnome-voice/data/screenshots/gnome-voice-screenshot.png</screenshot>
  </screenshots>
  <updatecontact>ole@aamot.org</updatecontact>
</application>
